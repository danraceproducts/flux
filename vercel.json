{
  "version": 2,
  "builds": [
    {
      "src": "packages/server/dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "packages/web/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "packages/server/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "packages/web/dist/$1"
    }
  ]
}
